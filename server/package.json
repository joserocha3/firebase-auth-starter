{
  "name": "functions",
  "scripts": {
    "serve": "yarn run prepare && firebase serve --only functions",
    "shell": "firebase experimental:functions:shell",
    "start": "npm run shell",
    "deploy": "yarn run prepare && firebase deploy --only functions",
    "logs": "firebase functions:log",
    "prepare": "babel src --out-dir functions --copy-files --ignore node_modules"
  },
  "dependencies": {
    "firebase-admin": "^5.12.0",
    "firebase-functions": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1"
  }
}
